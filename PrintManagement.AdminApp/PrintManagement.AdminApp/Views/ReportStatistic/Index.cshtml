    <a class="fa fa-bar-chart" style="color:green; margin-bottom:10px"></a>&nbsp;&nbsp;@Html.ActionLink("Báo cáo thống kê lưu lượng in ấn", "ReportPrintTrafficStatistic", "ReportStatistic")<br />
    <a class="fa fa-bar-chart" style="color:green; margin-bottom:10px"></a>&nbsp;&nbsp;@Html.ActionLink("Báo cáo thống kê chi in ấn", "ReportPrintingExpensesStatistic", "ReportStatistic")<br />
